    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.14.2",
    "father": "^2.29.2",
    "father-build": "^1.8.0",
    "np": "^5.0.3",
    "prettier": "^1.18.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "stylelint": "^9.10.1",
    "stylelint-config-ali": "^0.3.1",
    "stylelint-config-css-modules": "^1.5.0",
    "stylelint-config-prettier": "^5.3.0",
    "stylelint-scss": "^3.11.1",
    "tslint": "^5.18.0",
